{
  "name": "vrchat",
  "description": "Unofficial VRChat API",
  "version": "0.0.5",
  "author": "Marei Kikukawa <contact@calmery.me>",
  "bugs": "https://github.com/calmery/VRChatJS/issues",
  "dependencies": {
    "axios": "0.21.1",
    "cookie": "0.4.1"
  },
  "devDependencies": {
    "@types/cookie": "0.4.0",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "engine": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/calmery/VRChatJS",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "roots": [
      "<rootDir>/tests"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "keywords": [
    "vrchat"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "npm run prettify",
      "git add"
    ]
  },
  "main": "build",
  "repository": {
    "type": "git",
    "url": "https://github.com/calmery/VRChatJS.git"
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "prepare": "tsc",
    "prettify": "eslint --fix src/ tests/ --ext .ts",
    "test": "jest --coverage",
    "watch": "rimraf build && tsc --watch"
  }
}
